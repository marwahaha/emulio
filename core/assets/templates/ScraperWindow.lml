<:importStyleSheet>templates/ScrapperWindow.lss</:importStyleSheet>
<table id="scraperTable">
    <scrollPane id="scraperScroll">
        <list id="scraperList" selected="0">
            <:for itemName="{platformsNames}">
                {itemName}
            </:for>
        </list>
    </scrollPane>
    <separator></separator>
    <window id="scrapperWindow">
        <container>
            <image id="scraperPlatformImage"/>
        </container>
    </window>
</table>